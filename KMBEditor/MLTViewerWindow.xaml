<Window x:Class="KMBEditor.MLTViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KMBEditor"
        mc:Ignorable="d"
        Title="MLTViewerWindow" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="95*"/>
            <ColumnDefinition Width="197*"/>
        </Grid.ColumnDefinitions>
        <TreeView x:Name="treeView"/>
        <TextBlock x:Name="textBlock" Grid.Column="1" TextWrapping="Wrap" Text="TextBlock" FontFamily="MS PGothic" Background="#FFF0F0F0"/>

    </Grid>
</Window>
