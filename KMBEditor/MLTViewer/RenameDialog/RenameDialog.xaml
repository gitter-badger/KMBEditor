<Window x:Class="KMBEditor.MLTViewer.RenameDialog.RenameDialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KMBEditor.MLTViewer.RenameDialog"
        mc:Ignorable="d"
        Title="RenameDialog" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <TextBlock Text="タブのリネーム" />
            <TextBox Text="{Binding Path=InputText.Value, Mode=TwoWay}"/>
            <Button Content="OK" Command="{Binding Path=OkButtonClickCommand}"/>
        </StackPanel>
    </Grid>
</Window>
